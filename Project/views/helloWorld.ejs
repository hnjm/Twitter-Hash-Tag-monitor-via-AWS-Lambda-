    <body>
        
		
        <div class="container">
             <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>
	<p>Data: <%= twitter_data.length%></p>
	
	 <p>Data:</p>
	 <div class="panel panel-success">
  <!-- Default panel contents -->
  <div class="panel-heading"># tag name</div>
  <div class="panel-body">
    <table class="table table-hover table-bordered">
		<tr class="warning"><th>User Name</th><th>Tweet</th><th>GeoLocation</th></tr>
		<% twitter_data.forEach(function(twitter) { %>
			<tr><td><%= twitter.user_name %></td><td><%=  twitter.tweet_text %></td><td></td></tr>
		<% }); %>
	</table>
  </div>
	</div>
